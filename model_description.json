{
   "name":"Visuall Sample App",
   "icon": "assets/img/logo.png",
   "font": {
      "base": {
         "font-family": "'Open Sans', sans-serif",
         "font-size": "14px"
      },
      "heading": {
         "font-weight": "bold",
         "font-size": "16px"
      },
      "text": {}
   },
   "classes":{
      "person":{
         "properties":{
            "id":"number",
            "name":"string",
            "born":"number"
         },
         "style":{
            "label":"return ele.data('name')",
            "color": "white",
            "text-valign":"center",
            "text-wrap":"wrap",
            "text-max-width":"100px",
            "shape":"ellipse",
            "icon":"/img/person.png",
            "border-width":"2px",
            "border-color":"#ff6600",
            "background-color": "#ff9933",
            "width":"100px",
            "height":"100px"
         }
      },
      "movie":{
         "properties":{
            "id":"number",
            "title":"string",
            "tagline":"string",
            "released":"number"
         },
         "style":{
            "label":"return ele.data('title')",
            "color": "white",
            "text-valign": "center",
            "text-wrap": "wrap",
            "text-max-width": "100px",
            "shape":"ellipse",
            "icon":"/img/movie.png",
            "border-width":"2px",
            "border-color":"#0066ff",
            "background-color": "#33adff",
            "width":"100px",
            "height":"100px"
         }
      }
   },
   "relations":{
      "ACTED_IN":{
         "valid_ends":[
            "person",
            "movie"
         ],
         "properties":{
            "id":"number",
            "roles":"string"
         },
         "style":{
            "label":"return 'ACTED_IN'",
            "width":"2px",
            "line-color":"#339933",
            "line-style":"solid",
            "curve-style":"bezier",
            "target-arrow-shape":"triangle",
            "target-arrow-color":"#339933",
            "text-rotation":"autorotate",
            "text-outline-color":"white",
            "text-outline-width":"1"
         }
      },
      "DIRECTED":{
         "valid_ends":[
            "person",
            "movie"
         ],
         "properties":{
            "id":"number"
         },
         "style":{
            "label": "return 'DIRECTED'",
            "width":"2px",
            "line-color":"#808080",
            "line-style":"solid",
            "curve-style":"bezier",
            "target-arrow-shape":"triangle",
            "target-arrow-color":"#808080",
            "text-rotation":"autorotate",
            "text-outline-color":"white",
            "text-outline-width":"1"
         }
      },
      "PRODUCED":{
         "valid_ends":[
            "person",
            "movie"
         ],
         "properties":{
            "id":"number"
         },
         "style":{
           "label": "return 'PRODUCED'",
            "width":"2px",
            "line-color":"#808080",
            "line-style":"solid",
            "curve-style":"bezier",
            "target-arrow-shape":"triangle",
            "target-arrow-color":"#808080",
            "text-rotation":"autorotate",
            "text-outline-color":"white",
            "text-outline-width":"1"
         }
      }
   }
}