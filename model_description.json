{
   "name":"Turkcell Fraud Detection",
   "icon": "assets/img/logo.png",
   "font": {
      "base": {
         "font-family": "'Open Sans', sans-serif",
         "font-size": "14px"
      },
      "heading": {
         "font-weight": "bold",
         "font-size": "16px"
      },
      "text": {}
   },
   "classes":{
      "person":{
         "properties":{
            "id":"number",
            "first_name":"string",
            "last_name":"string",
            "standing":"boolean"
         },
         "style":{
            "label":"return ele.data('first_name') + ' ' + ele.data('last_name')",
            "shape":"ellipse",
            "icon":"/img/person.png",
            "border-width":"3px",
            "border-color":"red",
            "width":"50px",
            "height":"50px"
         }
      },
      "sim":{
         "properties":{
            "number":"string",
            "plan":"number"
         },
         "style":{
            "label":"return ele.data('number')",
            "shape":"rectangle",
            "icon":"/img/sim.png",
            "border-width":"3px",
            "border-color":"yellow",
            "width":"50px",
            "height":"50px"
         }
      },
      "dealer":{
         "properties":{
            "id":"number",
            "name":"string"
         },
         "style":{
            "label":"return ele.data('name')",
            "shape":"pentagon",
            "icon":"/img/dealer.png",
            "border-width":"5px",
            "border-color":"blue",
            "width":"50px",
            "height":"100px"
         }
      },
      "address":{
         "properties":{
            "street":"string",
            "apt_number":"number",
            "zip_code":"string",
            "city":"string",
            "country":"string"
         },
         "style":{
            "label":"return ele.data('street') + ' ' + ele.data('apt_number')",
            "shape":"rectangle",
            "icon":"/img/address.png",
            "border-width":"2px",
            "border-color":"black",
            "width":"50px",
            "height":"25px"
         }
      }
   },
   "relations":{
      "owns":{
         "valid_ends":[
            "person",
            "sim"
         ],
         "style":{
            "label": "return 'owns'",
            "width":"3px",
            "line-color":"black",
            "line-style":"solid",
            "source-arrow-shape":"none",
            "target-arrow-shape":"triangle"
         }
      },
      "lives-in":{
         "valid_ends":[
            "person",
            "address"
         ],
         "style":{
            "label": "return 'lives-in'",
            "width":"3px",
            "line-color":"red",
            "line-style":"solid",
            "source-arrow-shape":"none",
            "target-arrow-shape":"square"
         }
      },
      "is-sold-by":{
         "valid_ends":[
            "sim",
            "dealer"
         ],
         "style":{
           "label": "return 'is-sold-by'",
            "width":"3px",
            "line-color":"black",
            "line-style":"dashed",
            "source-arrow-shape":"none",
            "target-arrow-shape":"triangle"
         }
      },
      "is-in":{
         "valid_ends":[
            "dealer",
            "address"
         ],
         "style":{
           "label": "return 'is-in'",
            "width":"3px",
            "line-color":"blue",
            "line-style":"solid",
            "source-arrow-shape":"none",
            "target-arrow-shape":"square"
         }
      }
   }
}