  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "ng e2e",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\node_modules\\protractor\\bin\\protractor",
      "args": [
        "${workspaceFolder}\\e2e\\protractor.conf.js"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ng e2e mac",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
      "args": [
        "${workspaceFolder}/e2e/protractor.conf.js"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    }
  ]
}