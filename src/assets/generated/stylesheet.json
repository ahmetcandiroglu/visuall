[
  {
    "selector": ".hidden",
    "style": {
      "display": "none"
    }
  },
  {
    "selector": "node",
    "style": {
      "background-opacity": 0,
      "font-size": "13px",
      "text-valign": "center",
      "text-halign": "center",
      "background-color": "#555",
      "text-outline-color": "#555",
      "text-outline-width": "2px",
      "color": "#fff",
      "overlay-padding": "6px",
      "z-index": "10",
      "text-wrap": "wrap",
      "text-max-width": "58px"
    }
  },
  {
    "selector": "node:selected",
    "style": {
      "overlay-color": "#6c757d",
      "overlay-opacity": 0.3,
      "background-color": "#999999"
    }
  },
  {
    "selector": "edge:selected",
    "style": {
      "overlay-color": "#6c757d",
      "overlay-opacity": 0.3,
      "line-color": "#808080"
    }
  },
  {
    "selector": "edge",
    "style": {
      "font-size": "13px",
      "text-outline-color": "#ffffff",
      "text-outline-width": "2px"
    }
  },
  {
    "selector": "node.Cluster",
    "style": {
      "background-opacity": 0.1,
      "background-color": "#555",
      "shape": "barrel",
      "border-color": "black",
      "font-size": "9px"
    }
  },
  {
    "selector": "node.cy-expand-collapse-collapsed-node",
    "style": {
      "label": "data(name)",
      "font-size": "9px",
      "border-color": "black",
      "border-width": 1,
      "border-style": "dotted",
      "background-opacity": 0,
      "shape": "barrel"
    }
  },
  {
    "selector": ":compound",
    "style": {
      "padding": "5%"
    }
  },
  {
    "selector": "node.Person",
    "style": {
      "label": "data(name)",
      "color": "#e5ffb5",
      "text-valign": "center",
      "font-size": "11px",
      "shape": "ellipse",
      "background-fit": "contain",
      "background-image": "assets/img/person.svg",
      "width": "58px",
      "height": "58px"
    }
  },
  {
    "selector": "node.Movie",
    "style": {
      "label": "data(title)",
      "color": "#ffc5a8",
      "text-valign": "bottom",
      "font-size": "9px",
      "background-image": "assets/img/movie-roll.svg",
      "shape": "ellipse",
      "background-fit": "contain",
      "width": "mapData(rating, 0, 10, 20px, 60px)",
      "height": "mapData(rating, 0, 10, 20px, 60px)"
    }
  },
  {
    "selector": "edge.ACTED_IN",
    "style": {
      "label": "acted in",
      "width": "4px",
      "font-size": "9px",
      "line-color": "#abcaf2",
      "line-style": "solid",
      "curve-style": "bezier",
      "target-arrow-shape": "triangle",
      "target-arrow-color": "#abcaf2",
      "text-rotation": "autorotate"
    }
  },
  {
    "selector": "edge.DIRECTED",
    "style": {
      "label": "directed",
      "width": "3px",
      "font-size": "9px",
      "line-color": "#1eead1",
      "line-style": "solid",
      "curve-style": "bezier",
      "target-arrow-shape": "triangle",
      "target-arrow-color": "#1eead1",
      "text-rotation": "autorotate"
    }
  },
  {
    "selector": "edge.PRODUCED",
    "style": {
      "label": "produced",
      "width": "2px",
      "font-size": "9px",
      "line-color": "#ed9ca7",
      "line-style": "solid",
      "curve-style": "bezier",
      "target-arrow-shape": "tee",
      "target-arrow-color": "#ed9ca7",
      "text-rotation": "autorotate"
    }
  },
  {
    "selector": "edge.WROTE",
    "style": {
      "label": "wrote",
      "width": "2px",
      "font-size": "9px",
      "line-color": "#b5b74b",
      "line-style": "dashed",
      "curve-style": "bezier",
      "target-arrow-shape": "circle",
      "target-arrow-color": "#b5b74b",
      "text-rotation": "autorotate"
    }
  },
  {
    "selector": "edge.REVIEWED",
    "style": {
      "label": "reviewed",
      "width": "2px",
      "font-size": "9px",
      "line-color": "#f997fc",
      "line-style": "solid",
      "curve-style": "bezier",
      "target-arrow-shape": "triangle",
      "target-arrow-color": "#f997fc",
      "text-rotation": "autorotate"
    }
  },
  {
    "selector": "edge.FOLLOWS",
    "style": {
      "label": "follows",
      "width": "2px",
      "font-size": "9px",
      "line-color": "#f7a67e",
      "line-style": "dashed",
      "curve-style": "bezier",
      "target-arrow-shape": "triangle",
      "target-arrow-color": "#f7a67e",
      "text-rotation": "autorotate"
    }
  }
]